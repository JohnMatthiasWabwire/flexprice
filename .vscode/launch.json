    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch FlexPrice Server",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}",
            "env": {},
            "args": [],
            "showLog": true,
            "trace": "verbose"
        },
        {
            "name": "Launch FlexPrice Server (Debug Mode)",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/cmd/server/main.go",
            "cwd": "${workspaceFolder}",
            "env": {
                "GO_ENV": "development"
            },
            "args": [],
            "showLog": true,
            "trace": "verbose",
            "stopOnEntry": false,
            "console": "integratedTerminal"
        },
        {
            "name": "Attach to Process",
            "type": "go",
            "request": "attach",
            "mode": "local",
            "processId": 0
        }
    ]
}